<?xml version="1.0" encoding="UTF-8"?>
<page pageType="baidu_zhidao_list">
	<uielements objectName="beforeSemantic">
		<uiobject fieldName="tidyDom">
			<expression><![CDATA[$JSOUPDOC(html)]]></expression>
		</uiobject>
	</uielements>
	<uielements objectName="results">
		<rows><![CDATA[$JSOUPROWSDOC(tidyDom,"div[class=list]>dl[class=dl]")]]></rows>
		<uiobject fieldName="id" nullAble="false">
			<expression><![CDATA[$JSOUPATTRDOC(tidyDom,"div[class=list]>dl[class=dl]","href",row-1)]]></expression>
		</uiobject>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA[$JSOUPATTRDOC(tidyDom,"div[class=list]>dl[class=dl]","href",row-1)]]></expression>
		</uiobject>
		<uiobject fieldName="title" nullAble="false">
			<expression><![CDATA[$JSOUPTXTLISTDOC(tidyDom,"div[class*=c-container]>h3>a",row-1)]]></expression>
		</uiobject>
		<uiobject fieldName="thumbnail" nullAble="true">
			<expression><![CDATA[$JSOUPATTRDOC(tidyDom,"div[class*=c-container]:nth-child(GaodigJsoupRow)>div>div.general_image_pic>a","href",row)]]></expression>
		</uiobject>
		<uiobject fieldName="abstract" nullAble="true">
			<expression><![CDATA[$JSOUPTXTLISTDOC(tidyDom,"div[class*=c-container]>div[class^=c]",row-1)]]></expression>
		</uiobject>
		<uiobject fieldName="order" nullAble="true">
			<expression><![CDATA[row]]></expression>
		</uiobject>
		<uiobject fieldName="page_num" nullAble="false">
			<expression><![CDATA[page]]></expression>
		</uiobject>
		<uiobject fieldName="search_result_num" nullAble="true">
			<expression><![CDATA[$REGEXP($STR_REPLACE($JSOUPTXTLISTDOC(tidyDom,"div.nums",0),",",""),"(\\d+)")]]></expression>
		</uiobject>
	</uielements>
	<uielements objectName="nextTask">
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA[$MERGEURL($REGEXP($KEYVAL(task,"original_url"),"(http.*?://[^/]+)"),$JSOUPATTRDOC(tidyDom,"div#page>a.n",href,0))]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["webservice_baidu_search"]]></expression>
		</uiobject>
		<uiobject fieldName="bAjax" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bAjax")]]></expression>
		</uiobject>
		<uiobject fieldName="bProxy" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bProxy")]]></expression>
		</uiobject>
	</uielements>
	
</page>