<?xml version="1.0" encoding="UTF-8"?>
<page pageType="patent_list">
	<uielements objectName="beforeSemantic">
		<uiobject fieldName="tidyDom">
			<expression><![CDATA[$JSOUPDOC(html)]]></expression>
		</uiobject>
	</uielements>
	<uielements objectName="detailTask">
		<rows><![CDATA[$JSOUPROWSDOC(tidyDom,".list-container>ul>li")]]></rows>
		<uiobject fieldName="keyword" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"keyword")]]></expression>
		</uiobject>
		<uiobject fieldName="patent_id" nullAble="false">
			<expression><![CDATA[$JSOUPATTRDOC($CHINA_TMP(tidyDom,".list-container>ul>li",row-1),"input[name=vIdHidden]","value",0)]]></expression>
		</uiobject>
		<uiobject fieldName="title" nullAble="true">
			<expression><![CDATA[$JSOUPTXTLISTDOC($CHINA_TMP(tidyDom,".item-header.clear",row-1),"h1",0)]]></expression>
		</uiobject>
		<uiobject fieldName="tag" nullAble="true">
			<expression><![CDATA[$JSOUPTXTLISTDOC($CHINA_TMP(tidyDom,".item-header.clear",row-1),"div.btn-group.left.clear",0)]]></expression>
		</uiobject>
		<uiobject fieldName="only_id" nullAble="false">
			<expression><![CDATA[$JSOUPATTRDOC($CHINA_TMP(tidyDom,".list-container>ul>li",row-1),"input[name=idHidden]","value",0)]]></expression>
		</uiobject>
		<uiobject fieldName="nrdPN_id" nullAble="true">
