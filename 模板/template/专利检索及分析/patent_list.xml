		</uiobject>
	   <uiobject fieldName="url" nullAble="false">
			<expression><![CDATA["http://www.pss-system.gov.cn/sipopublicsearch/patentsearch/showAbstractInfo-viewAbstractInfo.shtml?nrdAn="+$KEYVAL(map,"patent_id")+"&cid="+$KEYVAL(map,"only_id")+"&sid="+$KEYVAL(map,"only_id")]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["patent_detail"]]></expression>
		</uiobject>
		<uiobject fieldName="bAjax" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bAjax")]]></expression>
		</uiobject>
		<uiobject fieldName="bAjax" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bAjax")]]></expression>
		</uiobject>
