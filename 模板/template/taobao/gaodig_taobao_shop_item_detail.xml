<?xml version="1.0" encoding="UTF-8"?>
<page pageType="gaodig_taobao_shop_item_detail">
	<uielements objectName="beforeSemantic">
		<uiobject fieldName="tidyDom">
			<expression><![CDATA[$TIDYDOMFMT($CLEAN(html,$KEYVAL(task,"url")))]]></expression>
		</uiobject>
	</uielements>
	<uielements objectName="tb_items">
		<uiobject fieldName="rowId:item_id" nullAble="false">
			<expression><![CDATA[$GETURLFIELD($KEYVAL(task,"url"),"id")]]></expression>
		</uiobject>
		<uiobject fieldName="item_attributes" nullAble="false">
			<expression><![CDATA[$XPATHROWS_KEEPTAGS(tidyDom,"//*[@id='attributes']/ul")]]></expression>
		</uiobject>
		<uiobject fieldName="update_time" nullAble="false">
			<expression><![CDATA[$SYSDATE()]]></expression>
		</uiobject>
	</uielements>
	<uielements objectName="ajax_Task">
		<uiobject fieldName="item_id" nullAble="false">
			<expression><![CDATA[$GETURLFIELD($KEYVAL(task,"url"),"id")]]></expression>
		</uiobject>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA["https://rate.taobao.com/detailCommon.htm?callback=jsonp_reviews_summary2&siteID=1&userNumId="+$KEYVAL(task,"seller_id")+"&auctionNumId="+$KEYVAL(map,"item_id")]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["gaodig_taobao_crawler_shop_item_review"]]></expression>
		</uiobject>
		<uiobject fieldName="referer" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"url")]]></expression>
		</uiobject>
		<uiobject fieldName="proxyType" nullAble="true">
			<expression><![CDATA["taobao"]]></expression>
		</uiobject>
	</uielements>
	<uielements objectName="ajax_Task">
		<uiobject fieldName="item_id" nullAble="false">
			<expression><![CDATA[$GETURLFIELD($KEYVAL(task,"url"),"id")]]></expression>
		</uiobject>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA["https://detailskip.taobao.com/json/sib.htm?u=1&p=1&chnl=pc&shopId=&vd=1&skil=false&pf=1&al=false&ap=0&ss=0&free=1&st=1&ct=1&prior=1&sellerId="+$KEYVAL(task,"seller_id")+"&itemId="+$KEYVAL(map,"item_id")]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["gaodig_taobao_crawler_shop_item_stock"]]></expression>
		</uiobject>
		<uiobject fieldName="referer" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"url")]]></expression>
		</uiobject>
		<uiobject fieldName="proxyType" nullAble="true">
			<expression><![CDATA["taobao"]]></expression>
		</uiobject>
	</uielements>
	<uielements objectName="ajax_Task">
		<uiobject fieldName="item_id" nullAble="false">
			<expression><![CDATA[$GETURLFIELD($KEYVAL(task,"url"),"id")]]></expression>
		</uiobject>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA[$GETFIELD(html,"apiItemInfo\":\"","\"")]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["gaodig_taobao_crawler_shop_item_trade"]]></expression>
