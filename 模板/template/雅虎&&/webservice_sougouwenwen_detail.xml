<?xml version="1.0" encoding="UTF-8"?>
<page pageType="webservice_sougouwenwen_detail">
	<uielements objectName="beforeSemantic">
		<uiobject fieldName="tidyDom">
			<expression><![CDATA[$JSOUPDOC(html)]]></expression>
		</uiobject>
	</uielements>

	<uielements objectName="results">
		<rows><![CDATA[$JSOUPROWSDOC(tidyDom,".answer-con")]]></rows>
		<uiobject fieldName="title" nullAble="false">
			<expression><![CDATA[$JSOUPTXTLISTDOC(tidyDom,"#questionTitle",0)]]></expression>
		</uiobject>

		<uiobject fieldName="description" nullAble="true">
			<expression><![CDATA[$JSOUPTXTLISTDOC(tidyDom,"#questionContent",0)]]></expression>
		</uiobject>
		<uiobject fieldName="content" nullAble="false">
			<expression><![CDATA[$JSOUPTXTLISTDOC(tidyDom,".answer-con",row-1)]]></expression>
		</uiobject>
		<uiobject fieldName="is_best_answer" nullAble="true">
			<expression><![CDATA["true"]]></expression>
		</uiobject>
		<uiobject fieldName="info_update_time" nullAble="true">
			<expression><![CDATA[$JSOUPTXTLISTDOC(tidyDom,".time",0)]]></expression>
		</uiobject>
	</uielements>

	<uielements objectName="results">
		<uiobject fieldName="title" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"question_title")]]></expression>
		</uiobject>
		<uiobject fieldName="content" nullAble="false">
			<expression><![CDATA[$EXTRACT_JSON($ARRAY($ARRAYFMT($EXTRACT_JSON($EXTRACT_JSON($JSONFMT(html),"data"),"answerList")),1),"content")]]></expression>
		</uiobject>
		<uiobject fieldName="is_best_answer" nullAble="true">
			<expression><![CDATA["true"]]></expression>
		</uiobject>
		<uiobject fieldName="info_update_time" nullAble="true">
			<expression><![CDATA[$EXTRACT_JSON($ARRAY($ARRAYFMT($EXTRACT_JSON($EXTRACT_JSON($JSONFMT(html),"data"),"answerList")),1),"time")]]></expression>
		</uiobject>
	</uielements>

	<uielements objectName="nextTask">
		<rows><![CDATA[$REGEXP($KEYVAL(task,"original_url"),"qid=(\\d+)")==""?0:1]]></rows>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA["http://wenwen.sogou.com/world/ajax/app/recanswerlist?id="+$REGEXP($KEYVAL(task,"original_url"),"qid=(\\d+)")]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["webservice_sougouwenwen_detail"]]></expression>
		</uiobject>
		<uiobject fieldName="question_title" nullAble="false">
			<expression><![CDATA[$JSOUPTXTLISTDOC(tidyDom,"#qTitle",0)]]></expression>
		</uiobject>
		<uiobject fieldName="bAjax" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bAjax")]]></expression>
		</uiobject>
		<uiobject fieldName="bProxy" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bProxy")]]></expression>
		</uiobject>
	</uielements>

</page>