			<expression><![CDATA[$JSOUPDOC(html)]]></expression>
		</uiobject>
	</uielements>
	<uielements objectName="emitperpage=zhihu_answers">
		<rows><![CDATA[$JSOUPROWSDOC(tidyDom,"div.zm-item-answer.zm-item-expanded")]]></rows>
		<uiobject fieldName="id" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"url")+row]]></expression>
		</uiobject>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"url")]]></expression>
		</uiobject>
		<uiobject fieldName="title" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"title")]]></expression>
		</uiobject>
		<uiobject fieldName="super_tag" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"super_tag")]]></expression>
		</uiobject>
		<uiobject fieldName="answer_abstract" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"answer_abstract")]]></expression>
		</uiobject>
		<uiobject fieldName="order" nullAble="true">
			<expression><![CDATA[row]]></expression>
		</uiobject>
		<uiobject fieldName="question_tag" nullAble="false">
			<expression><![CDATA[$JSOUPTXTLISTDOC(tidyDom,"div.zm-tag-editor-labels",0)]]></expression>
		</uiobject>
		<uiobject fieldName="question_detail" nullAble="true">
			<expression><![CDATA[$JSOUPTXTLISTDOC(tidyDom,"div.zm-editable-content",0)]]></expression>
		</uiobject>
		<uiobject fieldName="answer_detail" nullAble="true">
			<expression><![CDATA[$JSOUPTXTLISTDOC(tidyDom,"div.zm-editable-content.clearfix",row-1)]]></expression>
		</uiobject>
	</uielements>
</page>