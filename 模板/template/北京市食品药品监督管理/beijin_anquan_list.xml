<?xml version="1.0" encoding="UTF-8"?>
<page pageType="beijin_anquan_list">
	<uielements objectName="beforeSemantic">
		<uiobject fieldName="tidyDom">
			<expression><![CDATA[$JSOUPDOC(html)]]></expression>
		</uiobject>
	</uielements>

	<uielements objectName="documentTask">
		<rows><![CDATA[$JSOUPROWSDOC(tidyDom,".left>a")]]></rows>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA["http://www.bjda.gov.cn"+$JSOUPATTRDOC(tidyDom,".left>a","href",row-1)]]></expression>
		</uiobject>
		<uiobject fieldName="bAjax" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"bAjax")]]></expression>
		</uiobject>
		<uiobject fieldName="website" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"website")]]></expression>
		</uiobject>
		<uiobject fieldName="bProxy" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"bProxy")]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["beijin_anquan_detail"]]></expression>
		</uiobject>
	</uielements>	
	
	<uielements objectName="nextTask">
		<rows><![CDATA[(page+1)>$DIVIDED($REGEXP($JSOUPTXTLISTDOC(tidyDom,".Normal",0),"总记录数:(\\d+),"),25)?0:1]]></rows>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"code")+(page+1)+".html"]]></expression>
		</uiobject>
		<uiobject fieldName="bAjax" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bAjax")]]></expression>
		</uiobject>
		<uiobject fieldName="website" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"website")]]></expression>
		</uiobject>
		<uiobject fieldName="bProxy" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bProxy")]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["beijin_anquan_list"]]></expression>
		</uiobject>
	</uielements>
	
</page>