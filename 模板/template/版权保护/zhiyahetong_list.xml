<?xml version="1.0" encoding="UTF-8"?>
<page pageType="zhiyahetong_list">
	<uielements objectName="beforeSemantic">
		<uiobject fieldName="tidyDom">
			<expression><![CDATA[$JSOUPDOC(html)]]></expression>
		</uiobject>
	</uielements>
	<uielements objectName="emitperpage=banquan_zhiyahetong_info">
	<rows><![CDATA[$JSOUPROWSDOC($CHINA_TMP(tidyDom,"form[name=generalForm]>table",1),"tr")]]></rows>
		<uiobject fieldName="id" nullAble="false">
			<expression><![CDATA[$MD5($KEYVAL(task,"url")+row)]]></expression>
		</uiobject>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"url")]]></expression>
		</uiobject>
		<uiobject fieldName="register_id" nullAble="false">
			<expression><![CDATA[$JSOUPMYTEXT($JSOUPELMENT($CHINA_TMP(tidyDom,"form[name=generalForm]>table",1), "tr", row-1),"td", 0)]]></expression>
		</uiobject>
		<uiobject fieldName="chuzhiren" nullAble="true">
			<expression><![CDATA[$JSOUPMYTEXT($JSOUPELMENT($CHINA_TMP(tidyDom,"form[name=generalForm]>table",1), "tr", row-1),"td", 1)]]></expression>
		</uiobject>
		<uiobject fieldName="zhiquanren" nullAble="true">
			<expression><![CDATA[$JSOUPMYTEXT($JSOUPELMENT($CHINA_TMP(tidyDom,"form[name=generalForm]>table",1), "tr", row-1),"td", 2)]]></expression>
		</uiobject>
		<uiobject fieldName="zhiwuqingkuang" nullAble="true">
			<expression><![CDATA[$JSOUPMYTEXT($JSOUPELMENT($CHINA_TMP(tidyDom,"form[name=generalForm]>table",1), "tr", row-1),"td", 3)]]></expression>
		</uiobject>
		<uiobject fieldName="zhiya_register_date" nullAble="true">
			<expression><![CDATA[$JSOUPMYTEXT($JSOUPELMENT($CHINA_TMP(tidyDom,"form[name=generalForm]>table",1), "tr", row-1),"td", 4)]]></expression>
		</uiobject>
	</uielements>
	
	<uielements objectName="nextTask">
		<rows><![CDATA[$JSOUPATTRDOC(tidyDom,"a:contains(下一页)","href",0)==""?0:1]]></rows>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA["http://www.ccopyright.com.cn"+$JSOUPATTRDOC(tidyDom,"a:contains(下一页)","href",0)]]></expression>
		</uiobject>
		<uiobject fieldName="bAjax" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bAjax")]]></expression>
		</uiobject>
		<uiobject fieldName="bProxy" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bProxy")]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["zhiyahetong_list"]]></expression>
		</uiobject>
	</uielements>
	
