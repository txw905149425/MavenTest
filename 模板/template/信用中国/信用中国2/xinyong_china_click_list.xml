<?xml version="1.0" encoding="UTF-8"?>
<page pageType="xinyong_china_click_list">
	<uielements objectName="beforeSemantic">
		<uiobject fieldName="tidyDom">
			<expression><![CDATA[$JSOUPDOC(html)]]></expression>
		</uiobject>
	</uielements>

	<uielements objectName="documentTask">
		<rows><![CDATA[$JSOUPROWSDOC(tidyDom,".object-type>a")]]></rows>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA["http://www.creditchina.gov.cn"+$JSOUPATTRDOC(tidyDom,".object-type>a","href",row-1)]]></expression>
		</uiobject>
		<uiobject fieldName="website" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"website")]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["xinyong_china_click_detail"]]></expression>
		</uiobject>
		<uiobject fieldName="bAjax" nullAble="true">
			<expression><![CDATA["false"]]></expression>
		</uiobject>
		<uiobject fieldName="bProxy" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bProxy")]]></expression>
		</uiobject>
	</uielements>
	
	<uielements objectName="clickNextPageTask">
		<rows><![CDATA[$JSOUPTXTLISTDOC(tidyDom,".page-link.next",0)==""?0:1]]></rows>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA[$CLICKROW(tidyDom,".page-link.next",0)]]></expression>
		</uiobject>
		<uiobject fieldName="website" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"website")]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["xinyong_china_click_list"]]></expression>
		</uiobject>
		<uiobject fieldName="bAjax" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bAjax")]]></expression>
		</uiobject>
		<uiobject fieldName="bProxy" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bProxy")]]></expression>
		</uiobject>
	</uielements>
	

</page>