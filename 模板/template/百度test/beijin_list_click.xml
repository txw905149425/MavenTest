<?xml version="1.0" encoding="UTF-8"?>
<page pageType="baidu_list_click">
	<uielements objectName="beforeSemantic">
		<uiobject fieldName="tidyDom">
			<expression><![CDATA[$JSOUPDOC(html)]]></expression>
		</uiobject>
	</uielements>
	
	<uielements objectName="clickTask">
		<rows><![CDATA[$JSOUPROWSDOC(tidyDom,".ti")]]></rows>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA[$CLICKROW(tidyDom,".ti",row-1)]]></expression>
		</uiobject>
		<uiobject fieldName="title" nullAble="false">
			<expression><![CDATA[$JSOUPTXTLISTDOC(tidyDom,".ti",row-1)]]></expression>
		</uiobject>
		<uiobject fieldName="website" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"website")]]></expression>
		</uiobject>
		
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["baidu_detail_click"]]></expression>
		</uiobject>
	</uielements>
	
	<uielements objectName="clickTask">
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA[$CLICKROW(tidyDom,".pager-next",row-1)]]></expression>
		</uiobject>
		<uiobject fieldName="website" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"website")]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["baidu_list_click"]]></expression>
		</uiobject>
	</uielements>
	
</page>