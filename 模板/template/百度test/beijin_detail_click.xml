<?xml version="1.0" encoding="UTF-8"?>
<page pageType="baidu_detail_click">
	<uielements objectName="beforeSemantic">
		<uiobject fieldName="tidyDom">
			<expression><![CDATA[$JSOUPDOC(html)]]></expression>
		</uiobject>
	</uielements>
	<uielements objectName="emitperpage=a_a_baidu_test_detail">
		<uiobject fieldName="id" nullAble="false">
			<expression><![CDATA[$KEYVAL(curPageTask,"title")]]></expression>
		</uiobject>
		
		<uiobject fieldName="html" nullAble="false">
			<expression><![CDATA[html]]></expression>
		</uiobject>
	</uielements>
	
	<uielements objectName="clickTask">
		<rows><![CDATA[$JSOUPROWSDOC(tidyDom,".ec-img-list>a")]]></rows>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA[$CLICKROW(tidyDom,".ec-img-list>a",row-1)]]></expression>
		</uiobject>
		<uiobject fieldName="title" nullAble="false">
			<expression><![CDATA[$JSOUPTXTLISTDOC(tidyDom,".ec-img-list>a",row-1)]]></expression>
		</uiobject>
		<uiobject fieldName="website" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"website")]]></expression>
		</uiobject>
		
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["baidu_deep_click"]]></expression>
		</uiobject>
	</uielements>
	
	
	<uielements objectName="instantClickTask">
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA["click:back"]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["do_nothing"]]></expression>
		</uiobject>
	</uielements>
</page>