<?xml version="1.0" encoding="UTF-8"?>
<page pageType="judge_document_list">
	<uielements objectName="beforeSemantic">
		<uiobject fieldName="tidyDom">
			<expression><![CDATA[$ARRAYFMT(html)]]></expression>
		</uiobject>
	</uielements>
	
	<uielements objectName="emitperpage=judge_list_raw">
		<bvalid><![CDATA[$STR_LENGTH(html)>200]]></bvalid>
		<uiobject fieldName="id" nullAble="false">
			<expression><![CDATA[$KEYVAL($ARRAY(tidyDom,2),"文书ID")]]></expression>
		</uiobject>
		<uiobject fieldName="case_id" nullAble="false">
			<expression><![CDATA[$KEYVAL($ARRAY(tidyDom,2),"案号")]]></expression>
		</uiobject>
		<uiobject fieldName="company_name" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"company_name")]]></expression>
		</uiobject>
		<uiobject fieldName="website" nullAble="false">
			<expression><![CDATA["裁判文书网---list页面"]]></expression>
		</uiobject>
		<uiobject fieldName="html" nullAble="false">
			<expression><![CDATA[html]]></expression>
		</uiobject>
	</uielements>
	
	
	<uielements objectName="detailTask"> 
		<rows><![CDATA[$ROWSARRAY(tidyDom)]]></rows>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA["http://wenshu.court.gov.cn/CreateContentJS/CreateContentJS.aspx?DocID="+$KEYVAL($ARRAY(tidyDom,row+1),"文书ID")]]></expression>
		</uiobject>
		<uiobject fieldName="judge_id" nullAble="false">
			<expression><![CDATA[$KEYVAL($ARRAY(tidyDom,row+1),"文书ID")]]></expression>
		</uiobject>
		<uiobject fieldName="checkRepeat" nullAble="true">
			<expression><![CDATA["I##"+$KEYVAL(map,"url")+"##mongodb|judge_detail_raw|id"]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["judge_document_detail"]]></expression>
		</uiobject>
		<uiobject fieldName="company_name" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"company_name")]]></expression>
		</uiobject>
		<uiobject fieldName="bAjax" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bAjax")]]></expression>
		</uiobject>
		<uiobject fieldName="bProxy" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bProxy")]]></expression>
		</uiobject>
	</uielements>
	
	<uielements objectName="nextTask">
	     <rows><![CDATA[(page+1)>$DIVIDED($KEYVAL($ARRAY(tidyDom,1),"Count"),"20")?0:1]]></rows>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"url")]]></expression>
		</uiobject>
		<uiobject fieldName="listParam" nullAble="false">
			<expression><![CDATA["Param@全文检索:"+$KEYVAL(task,"company_name")+";Page@20;Order@法院层级;Direction@asc;Index@"+(page+1)]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["judge_document_list"]]></expression>
		</uiobject>
		<uiobject fieldName="bAjax" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bAjax")]]></expression>
		</uiobject>
		<uiobject fieldName="bProxy" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bProxy")]]></expression>
		</uiobject>
		<uiobject fieldName="isGetMethod" nullAble="true">
			<expression><![CDATA["false"]]></expression>
		</uiobject>
		<uiobject fieldName="company_name" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"company_name")]]></expression>
		</uiobject>
	</uielements>
	
</page>