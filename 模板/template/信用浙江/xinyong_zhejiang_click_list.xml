<?xml version="1.0" encoding="UTF-8"?>
<page pageType="xinyong_zhejiang_click_list">
	<uielements objectName="beforeSemantic">
		<uiobject fieldName="tidyDom">
			<expression><![CDATA[$JSOUPDOC(html)]]></expression>
		</uiobject>
	</uielements>

	<uielements objectName="clickTask">
		<rows><![CDATA[$JSOUPROWSDOC(tidyDom,".default_pgContainer>table>tbody>tr>td>a")]]></rows>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA[$CLICKROW(tidyDom,".default_pgContainer>table>tbody>tr>td>a",row-1)]]></expression>
		</uiobject>
		<uiobject fieldName="website" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"website")]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["xinyong_zhejiang_click_detail"]]></expression>
		</uiobject>
	</uielements>
	
	<uielements objectName="clickNextPageTask">
		<rows><![CDATA[page>$STR2INT($JSOUPTXTLISTDOC(tidyDom,".default_pgTotalPage",0))?0:1]]></rows>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA[$CLICKROW(tidyDom,".default_pgBtn.default_pgNext",0)]]></expression>
		</uiobject>
		<uiobject fieldName="website" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"website")]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["xinyong_zhejiang_click_list"]]></expression>
		</uiobject>
	</uielements>

</page>