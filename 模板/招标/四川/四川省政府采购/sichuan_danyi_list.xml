<?xml version="1.0" encoding="UTF-8"?>
<page pageType="sichuan_danyi_list">
	<uielements objectName="beforeSemantic">
		<uiobject fieldName="tidyDom">
			<expression><![CDATA[$JSOUPDOC(html)]]></expression>
		</uiobject>
	</uielements>
	<uielements objectName="emitperpage=txw_bid_sc_danyi_list_raw">
		<uiobject fieldName="id" nullAble="false">
			<expression><![CDATA[$KEYVAL(curPageTask,"url")]]></expression>
		</uiobject>
		<uiobject fieldName="title" nullAble="false">
			<expression><![CDATA[$JSOUPTXTLISTDOC(tidyDom,"div.colsList>ul>li>a",0)]]></expression>
		</uiobject>
		<uiobject fieldName="html" nullAble="false">
			<expression><![CDATA[html]]></expression>
		</uiobject>
		<uiobject fieldName="website" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"website")]]></expression>
		</uiobject>
	</uielements>
	
	<uielements objectName="documentTask">
		<rows><![CDATA[$JSOUPROWSDOC(tidyDom,"div.colsList>ul>li>a")]]></rows>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA["http://www.sczfcg.com"+$JSOUPATTRDOC(tidyDom,"div.colsList>ul>li>a","href",row-1)]]></expression>
		</uiobject>
		<uiobject fieldName="website" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"website")]]></expression>
		</uiobject>
		<uiobject fieldName="bAjax" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"bAjax")]]></expression>
		</uiobject>
		<uiobject fieldName="bProxy" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bProxy")]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["sichuan_danyi_detail"]]></expression>
		</uiobject>
		<uiobject fieldName="checkRepeat" nullAble="true">
			<expression><![CDATA["I##"+$KEYVAL(map,"url")+"##mongodb|txw_bid_sc_danyi_detail_raw|id"]]></expression>
		</uiobject>
	</uielements>
	
	<uielements objectName="nextTask">
		<rows><![CDATA[(page+1)>$STR2INT($REGEXP($JSOUPTXTLISTDOC(tidyDom,"div.flipPage",0),"/(\\d+)"))?0:1]]></rows>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"tmp")+(page+1)]]></expression>
		</uiobject>
		<uiobject fieldName="website" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"website")]]></expression>
		</uiobject>
		<uiobject fieldName="tmp" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"tmp")]]></expression>
		</uiobject>
		<uiobject fieldName="bAjax" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bAjax")]]></expression>
		</uiobject>
		<uiobject fieldName="bProxy" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bProxy")]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["sichuan_danyi_list"]]></expression>
		</uiobject>
	</uielements>
	
</page>