<?xml version="1.0" encoding="UTF-8"?>
<page pageType="chongqing_caigou_list">
	<uielements objectName="beforeSemantic">
		<uiobject fieldName="tidyDom">
			<expression><![CDATA[$JSONFMT(html)]]></expression>
		</uiobject>
	</uielements>
	
	<uielements objectName="emitperpage=txw_bid_cq_caigou_list_raw">
		<uiobject fieldName="id" nullAble="false">
			<expression><![CDATA[$KEYVAL(curPageTask,"url")]]></expression>
		</uiobject>
		<uiobject fieldName="title" nullAble="false">
			<expression><![CDATA[$KEYVAL($ARRAY($KEYVAL(tidyDom,"notices"),1),"id")]]></expression>
		</uiobject>
		<uiobject fieldName="website" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"website")]]></expression>
		</uiobject>
		<uiobject fieldName="html" nullAble="false">
			<expression><![CDATA[html]]></expression>
		</uiobject>
	</uielements>
	
	<uielements objectName="detailTask">
		<rows><![CDATA[$ROWSARRAY($KEYVAL(tidyDom,"notices"))]]></rows>
		<uiobject fieldName="tp1" nullAble="false">
			<expression><![CDATA[$KEYVAL($ARRAY($KEYVAL(tidyDom,"notices"),row),"id")]]></expression>
		</uiobject>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA["https://www.cqgp.gov.cn/gwebsite/api/v1/notices/stable/"+$KEYVAL(map,"tp1")+"?timestamp="+$SYSDATELONG()]]></expression>
		</uiobject>
		<uiobject fieldName="website" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"website")]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["chongqing_caigou_detail"]]></expression>
		</uiobject>
		<uiobject fieldName="bAjax" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bAjax")]]></expression>
		</uiobject>
		<uiobject fieldName="checkRepeat" nullAble="true">
			<expression><![CDATA["I##"+$KEYVAL(map,"url")+"##mongodb|txw_bid_cq_caigou_detail_raw|id"]]></expression>
		</uiobject>
		<uiobject fieldName="bProxy" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bProxy")]]></expression>
		</uiobject>
	</uielements>
	
	<uielements objectName="nextTask">
		<rows><![CDATA[(page+1)>$DIVIDED($KEYVAL(tidyDom,"total"),"20")?0:1]]></rows>
		<uiobject fieldName="url" nullAble="false">
			<expression><![CDATA[$KEYVAL(task,"tmp")+(page+1)]]></expression>
		</uiobject>
		<uiobject fieldName="website" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"website")]]></expression>
		</uiobject>
		<uiobject fieldName="tmp" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"tmp")]]></expression>
		</uiobject>
		<uiobject fieldName="bAjax" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bAjax")]]></expression>
		</uiobject>
		<uiobject fieldName="bProxy" nullAble="true">
			<expression><![CDATA[$KEYVAL(task,"bProxy")]]></expression>
		</uiobject>
		<uiobject fieldName="pageType" nullAble="false">
			<expression><![CDATA["chongqing_caigou_list"]]></expression>
		</uiobject>
	</uielements>
	
</page>
